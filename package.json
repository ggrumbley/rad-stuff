{
  "name": "rad-stuff",
  "version": "1.0.0",
  "scripts": {
    "dev": "next -p 7777",
    "build": "next build",
    "start": "next start",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json server",
    "db:migrate": "npm -s run prisma:save && npm -s run prisma:up",
    "prisma:save": "prisma migrate save --name init --experimental",
    "prisma:up": "prisma migrate up --experimental",
    "postinstall": "npm -s run generate",
    "tsc": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.50",
    "@apollo/react-ssr": "^3.1.5",
    "@nexus/schema": "^0.13.1",
    "@prisma/client": "^2.0.0-beta.7",
    "apollo-client": "^2.6.10",
    "apollo-server-micro": "^2.14.0",
    "date-fns": "^2.14.0",
    "downshift": "^5.4.2",
    "graphql": "^15.0.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tools": "^6.0.3",
    "lodash.debounce": "^4.0.8",
    "next": "^9.4.2",
    "nexus-prisma": "^0.13.0",
    "nprogress": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-stripe-checkout": "^2.6.3",
    "react-transition-group": "^4.4.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@prisma/cli": "^2.0.0-beta.7",
    "@types/graphql": "^14.5.0",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "5.0.1",
    "babel-plugin-graphql-tag": "^2.5.0",
    "babel-plugin-styled-components": "^1.10.7",
    "typescript": "^3.9.3"
  },
  "license": "ISC"
}
